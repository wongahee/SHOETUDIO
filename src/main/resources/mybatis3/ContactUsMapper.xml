<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="contactus">

    <insert id="insertContact" statementType="PREPARED"
            parameterType="SHOETUDIO.spring.mvc.vo.ContactUs">
        insert into contactus (name, email, findus, message)
        values (#{name}, #{email}, #{findus}, #{message})
    </insert>

</mapper>